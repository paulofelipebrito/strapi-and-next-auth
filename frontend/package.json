{
  "name": "course-graphql-next-strapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "test": "jest",
    "deploy-static": "npm run build && npm run export",
    "lint": "eslint src --max-warnings=0",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "fft:components": "files-from-template --config-files='.fft/config' --root='components'",
    "fft:templates": "files-from-template --config-fWiles='.fft/config' --root='templates'"
  },
  "dependencies": {
    "@styled-icons/material-outlined": "^10.28.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "next": "^10.1.2",
    "next-auth": "^3.13.3",
    "nextjs-progressbar": "0.0.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.2.0",
    "@storybook/addon-essentials": "^6.2.0",
    "@storybook/addon-links": "^6.2.0",
    "@storybook/react": "^6.2.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "files-from-template": "^1.0.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run lint -- --fix",
      "npm test -- --findRelatedTests --bail"
    ]
  }
}
